{
  "name": "@marketto/codice-fiscale-utils",
  "version": "1.1.3",
  "description": "JS utilities to handle Italian Codice Fiscale",
  "main": "dist/codice-fiscale-utils.cjs.min.js",
  "module": "src/index.js",
  "browser": "dist/codice-fiscale-utils.amd.min.js",
  "scripts": {
    "test": "npx nyc npx mocha",
    "coverage": "mkdir coverage \n npm run test && npx nyc report --reporter=text-lcov | npx relcov > coverage/lcov.info",
    "build": "npx rimraf ./dist && npx webpack",
    "docs": "npx rimraf ./docs && npx jsdoc . -c jsdoc.json",
    "update-lovs": "node ./scripts/locationUpdater.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marketto/codice-fiscale-utils.git"
  },
  "keywords": [
    "CF",
    "CodiceFiscale",
    "Codice Fiscale",
    "Fiscal Code",
    "Generator",
    "Parser",
    "Validator",
    "Omocodia",
    "Omocode",
    "Belfiore"
  ],
  "author": "Marco Ricupero <marco.ricupero@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marketto/codice-fiscale-utils/issues"
  },
  "homepage": "https://github.com/Marketto/codice-fiscale-utils#readme",
  "dependencies": {
    "@marketto/diacritic-remover": "^1.0.3",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-things": "^0.2.0",
    "csvtojson": "^2.0.10",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.3",
    "jszip": "^3.2.2",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "relcov": "^1.0.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "sonarqube-scanner": "^2.5.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  }
