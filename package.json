{
  "name": "@marketto/codice-fiscale-utils",
  "version": "1.2.2",
  "description": "TS & JS utilities to handle Italian Codice Fiscale",
  "main": "dist/codice-fiscale-utils.js",
  "module": "dist/codice-fiscale-utils.mjs",
  "typings": "src/index.ts",
  "scripts": {
    "release": "npm audit fix \n npm run lint-fix && npm run update-lovs && npm run test && npm run coverage \n npm run docs \n npm run build",
    "build": "npx rimraf ./dist \n npx rollup -c",
    "lint": "npx tslint -c tslint.json \"src/**/*.ts\"",
    "lint-fix": "npx tslint -c tslint.json \"src/**/*.ts\" --fix",
    "test": "npx mocha",
    "coverage": "npx rimraf ./coverage \n mkdir coverage \n npx nyc mocha \n npx nyc report --reporter=text-lcov | npx relcov > coverage/lcov.info",
    "docs": "npx rimraf ./docs \n npx jsdoc . -c jsdoc.json",
    "update-lovs": "node ./scripts/locationUpdater.js",
    "install": "node ./scripts/post-install.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marketto/codice-fiscale-utils.git"
  },
  "keywords": [
    "CF",
    "CodiceFiscale",
    "Codice Fiscale",
    "Fiscal Code",
    "Generator",
    "Parser",
    "Validator",
    "Omocodia",
    "Omocode",
    "Belfiore",
    "TypeScript"
  ],
  "author": "Marco Ricupero <marco.ricupero@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marketto/codice-fiscale-utils/issues"
  },
  "homepage": "https://github.com/Marketto/codice-fiscale-utils#readme",
  "dependencies": {
    "@marketto/diacritic-remover": "^1.0.6",
    "moment": "^2.24.0",
    "read-excel-file": "^4.0.5"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@rollup/plugin-json": "^4.0.0",
    "@types/chai": "^4.2.5",
    "@types/chai-string": "^1.4.2",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@wessberg/rollup-plugin-ts": "^1.1.83",
    "chai": "^4.2.0",
    "chai-string": "^1.5.0",
    "colors": "^1.4.0",
    "cross-env": "^6.0.3",
    "csvtojson": "^2.0.10",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.3",
    "jszip": "^3.2.2",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "relcov": "^1.0.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.0",
    "rollup": "^1.26.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-license": "^0.12.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "sonarqube-scanner": "^2.5.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  }
}
