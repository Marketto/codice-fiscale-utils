{
  "name": "@marketto/codice-fiscale-utils",
  "version": "1.1.3",
  "description": "JS utilities to handle Italian Codice Fiscale",
  "_type": "module",
  "main": "dist/codice-fiscale-utils.cjs.min.js",
  "module": "src/index.js",
  "scripts": {
    "test": "npx cross-env NODE_ENV=test npx nyc npx mocha",
    "lint": "npx eslint ./src/**/*.js",
    "lint-fix": "npm run lint --fix",
    "coverage": "mkdir coverage \n npm run test && npx nyc report --reporter=text-lcov | npx relcov > coverage/lcov.info",
    "build": "npm run lint && npx rimraf ./dist && npx rollup -c",
    "docs": "npx rimraf ./docs && npx jsdoc . -c jsdoc.json",
    "update-lovs": "node ./scripts/locationUpdater.js",
    "release": "npm audit fix && npm run update-lovs && npm run test && npm run docs && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marketto/codice-fiscale-utils.git"
  },
  "keywords": [
    "CF",
    "CodiceFiscale",
    "Codice Fiscale",
    "Fiscal Code",
    "Generator",
    "Parser",
    "Validator",
    "Omocodia",
    "Omocode",
    "Belfiore"
  ],
  "author": "Marco Ricupero <marco.ricupero@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marketto/codice-fiscale-utils/issues"
  },
  "homepage": "https://github.com/Marketto/codice-fiscale-utils#readme",
  "dependencies": {
    "@marketto/diacritic-remover": "^1.0.3",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-modules-commonjs": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.7.4",
    "@babel/runtime": "^7.6.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-things": "^0.2.0",
    "core-js": "3",
    "cross-env": "^6.0.3",
    "csvtojson": "^2.0.10",
    "eslint": "^6.7.2",
    "foodoc": "0.0.9",
    "jsdoc": "^3.6.3",
    "jszip": "^3.2.2",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "relcov": "^1.0.6",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.3",
    "rollup": "^1.27.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-license": "^0.13.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "sonarqube-scanner": "^2.5.0"
  }
}
