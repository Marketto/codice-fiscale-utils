{
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "exclude": [
            "node_modules",
            "dist",
            "docs",
            "test",
            "scripts",
            "test",
            "demo"
        ]
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "theme_opts": {
            "default_theme": "dark"
        },
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    },
    "templates": {
        "systemName": "Codice Fiscale Utils",
        "systemSummary": "JS utilities to handle Italian Codice Fiscale"
    }
}